{
  "extends": [
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["react", "react-hooks", "prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "parser": "babel-eslint",
  "ignorePatterns": ["/node_modules/**", "build/**"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
